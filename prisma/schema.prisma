datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  username  String   @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}


model KetuaMajelis {
  id        Int @id @default(autoincrement())
  name      String
}

model PaniteraPengganti {
  id        Int @id @default(autoincrement())
  name      String
}